{
  "landingPage": "/wp-admin/options-general.php?page=wp-reportpanel-sdd-settings",
  "steps": [
    {
      "step": "setSiteOptions",
      "options": {
        "blogname": "Report Panel",
        "blogdescription": ""
      }
    },
    {
      "step": "defineWpConfigConsts",
      "consts": {
        "WP_DEBUG": true
      },
      "method": "rewrite-wp-config"
    },
    {
      "step": "runPHP",
      "code": "<?php include '/var/www/html/wp-load.php'; global $wp_rewrite; $wp_rewrite->set_permalink_structure('/%postname%/'); $wp_rewrite->flush_rules();"
    },
    {
      "step": "login",
      "username": "admin"
    },
    {
      "step": "activatePlugin",
      "pluginName": "Report Panel",
      "pluginPath": "/var/www/html/wp-content/plugins/wp-reportpanel-sdd"
    }
  ]
}
